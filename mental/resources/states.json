{
  "name": "states",
  "label": "States",
  "attributes": [
    {
      "identifier": "uuid",
      "label": "UUID",
      "operations": {
        "create": {
          "generate": "uuid",
          "validate": [
            { "type": "required" },
            {
              "type": "regex",
              "value": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
            }
          ]
        }
      }
    },
    {
      "identifier": "name",
      "label": "Name",
      "operations": {
        "create": {
          "validate": [
            { "type": "required" },
            { "type": "within", "value": ["Telangana", "Andhra Pradesh"] }
          ]
        }
      }
    },
    {
      "identifier": "slug",
      "label": "Slug"
    },
    {
      "identifier": "created_at",
      "label": "created_at",
      "operations": {
        "create": {
          "generate": "datetime"
        }
      }
    },
    {
      "identifier": "updated_at",
      "label": "updated_at",
      "operations": {
        "create": {
          "generate": "datetime"
        }
      }
    },
    {
      "identifier": "deleted_at",
      "label": "deleted_at"
    }
  ]
}
